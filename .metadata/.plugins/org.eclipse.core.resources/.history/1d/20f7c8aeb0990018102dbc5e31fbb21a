
public class DemoString {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		String name ="citi";
		System.out.println("Initial name: "+name);
	/*	name=name.concat(",Pune");
		System.out.println("Final name: "+name);
		*/
		String name2=new String("citi");
		String name1 = new String("                citicorp pvt ltd, Pune          ");
		if(name2==name) {
			System.out.println("Equal");
		}
		else {
			System.out.println("Not Equal");
		}
				
		System.out.println("Substring: "+name.substring(2, 4));
		
		System.out.println("Compare to: "+name.compareTo(name1));
		
		System.out.println("Contains: "+name.contains("ity"));
		
		System.out.println(""+name.length());
		
		System.out.println(""+name.charAt(3));
		
		
		System.out.println("Original"+name1);
		System.out.println(""+name1.trim());
		
		
	}

}

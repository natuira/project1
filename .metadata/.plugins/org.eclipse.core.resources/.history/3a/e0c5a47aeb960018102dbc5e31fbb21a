
public class DemoJagged {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int [][] values = new int [3][];
		
		values[0]=new int [4];
		values[0][0]=123;
		values[0][1]=124;
		values[0][2]=125;
		values[0][3]=126;
		
		values[1]=new int [2];
		values[1][0]=223;
		values[1][1]=224;
		
		values[2]=new int [5];
		
		
	}

}

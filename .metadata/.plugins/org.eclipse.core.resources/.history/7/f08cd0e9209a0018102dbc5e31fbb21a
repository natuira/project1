package com.pojo;

public class Student {

	private int rollNo;
	private int total;
	private String name;
	
	public Student() {
		// TODO Auto-generated constructor stub
	}

	public Student(int rollNo, int total, String name) {
		super();
		this.rollNo = rollNo;
		this.total = total;
		this.name = name;
	}
	
	
}

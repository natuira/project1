
public class DemoStatic {

	private float age;
	private String name;
	
	private static int value;
	
	public DemoStatic()
	{
		age=12.12f;
		name="abcd";
	}
	
	static {
		value=0;
	}
	
	public void display() {
		System.out.println("Name: "+name+"\tAge: "+age+"\tValue: "+value);
	}
	
	public static void show() {
		System.out.println("Name: "+name+"\tAge: "+"\tValue: "+value);
		
	}
	
	
}

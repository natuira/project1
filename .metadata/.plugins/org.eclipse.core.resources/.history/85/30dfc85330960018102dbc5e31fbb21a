
public class Person {

	private int age;
	private String name;

	public Person()
	{
		age=10;
		name="Ira";
	}
	
	public Person(int a, String n) {
		age=a;
		name=n;
	}

	public void display() {
		System.out.println("Name: "+name+"\tAge: "+age+"\n");
	}
	
	public void setValues() {
		
		age=21;
		name="Ira";
	}
	
	public void setValues(int a, String n) {
		
		age=a;
		name=n;
		
	}
	
	public void setAge(int a) {
		age=a;
	}
	public void setName(String n) {
		name=n;
	}
}

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int [] numbers;
		numbers = new int[3];
		
		numbers[0]=100;
		numbers[1]=200;
		numbers[2]=100;
		numbers[3]

		System.out.println("Array[0]: "+numbers[0] );
		System.out.println("Array[1]: "+numbers[1] );
		System.out.println("Array[2]: "+numbers[2] );
	}
